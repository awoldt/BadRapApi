<div class="row" style="margin-top: 25px;">
  <h3 class="text-center">Other artists</h3>
  <% for(var i=0; i<related_data.length; ++i) { %>

  <div class="col text-center">
    <a href='/artists/<%= related_data[i].artist_query %>' style="text-decoration: none;">
      <% if(related_data[i].artist_img !== null) { %>
      <img src='<%= related_data[i].artist_img %>' style="width: 75px; height: 75px;" alt="<%= related_data[i].artist_name %>" />
      <% } else { %>
      <img src='/no-img.png' style="width: 75px; height: 75px;" />
      <% } %>
      <p><%= related_data[i].artist_name %></p>
    </a>

  </div>

  <% } %>
</div>