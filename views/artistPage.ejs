      </div>



      <div class="col-lg-7">


        <hr>

        <% if(has_explicit_song.length !==0) { %>
        <div class="alert alert-danger p-3" role="alert"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" className="bi bi-explicit" viewBox="0 0 16 16">
            <path d="M6.826 10.88H10.5V12h-5V4.002h5v1.12H6.826V7.4h3.457v1.073H6.826v2.408Z" />
            <path d="M2.5 0A2.5 2.5 0 0 0 0 2.5v11A2.5 2.5 0 0 0 2.5 16h11a2.5 2.5 0 0 0 2.5-2.5v-11A2.5 2.5 0 0 0 13.5 0h-11ZM1 2.5A1.5 1.5 0 0 1 2.5 1h11A1.5 1.5 0 0 1 15 2.5v11a1.5 1.5 0 0 1-1.5 1.5h-11A1.5 1.5 0 0 1 1 13.5v-11Z" />
          </svg>
          <p class="m-0" style="display: inline;">Some songs contain explicit lyrics</p>
        </div>

        <% } %>



        <div>
          <%  if(artist_data.length == 1) { %>
          <h2> <code style="color: black"><%= artist_data.length %> lyric</code></h2>
          <% } else { %>
          <h2> <code style="color: black"> <%= artist_data.length %> of <%= artist_data[0].artist %>'s Worst lyrics</code></h2>
          <% }   %>

          <% if(artist_data.length == 1) { %>
          <p> <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-calendar" viewBox="0 0 16 16">
              <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z" />
            </svg> Lyric from <a href='/year/<%= lyrics_years_range[0] %>'><%= lyrics_years_range[0] %></a></p>
          <% } else { %>
          <p> <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-calendar" viewBox="0 0 16 16">
              <path d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z" />
            </svg> Lyrics from <% for(var i=0; i<lyrics_years_range.length; ++i) { %>
            <% if(i == lyrics_years_range.length-1) { %>
            <a href='/year/<%= lyrics_years_range[i] %>'><%= lyrics_years_range[i] %></a>
            <% } else { %>
            <a href='/year/<%= lyrics_years_range[i] %>'><%= lyrics_years_range[i] %></a> &middot
            <% } %>
            <% } %>
          </p>
          <% } %>

          <div style='background-color: rgb(242, 242, 242, .5); padding: 3vw; border-radius: 10px;'>
            <% for(var i=0; i<artist_data.length; ++i) { %>
            <div style="background-color: rgb(230, 230, 230, .5); padding: 10px; margin-bottom: 10px; border-radius: 5px; border: 1px solid black;" itemscope itemtype="https://schema.org/MusicComposition">
              <% if(artist_data[i].explicit) { %>
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" className="bi bi-explicit" viewBox="0 0 16 16">
                <path d="M6.826 10.88H10.5V12h-5V4.002h5v1.12H6.826V7.4h3.457v1.073H6.826v2.408Z" />
                <path d="M2.5 0A2.5 2.5 0 0 0 0 2.5v11A2.5 2.5 0 0 0 2.5 16h11a2.5 2.5 0 0 0 2.5-2.5v-11A2.5 2.5 0 0 0 13.5 0h-11ZM1 2.5A1.5 1.5 0 0 1 2.5 1h11A1.5 1.5 0 0 1 15 2.5v11a1.5 1.5 0 0 1-1.5 1.5h-11A1.5 1.5 0 0 1 1 13.5v-11Z" />
              </svg>

              <% } %>
              <p style="display: inline; font-size: 14px;"><i style="font-size: 20px;">"<span itemprop="lyrics"><%= artist_data[i].lyrics %></span>"</i>
                (<span itemprop="name"><%= artist_data[i].song %></span>, <span itemprop="copyrightYear"><%= artist_data[i].release_date %></span>)</p>

              <% if(artist_data[i].explanation !== undefined) { %>
              <div style="padding: 10px; background-color: rgb(255, 255, 0, .5); margin-top: 10px;">
                <span><u>Explanation:</u></span>
                <p style="margin-bottom: 0px;" itemprop="comment"><%= artist_data[i].explanation %></p>
              </div>
              <% } %>

            </div>
            <% } %>
          </div>
        </div>

        <% if(related_artists !== undefined) { %>
        <hr>
        </hr>
        <%- include('./relatedArtists', {related_data: related_artists}) %>
        <% } %>


      </div>



    </div>





  </div>
</body>

